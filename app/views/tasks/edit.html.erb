<%= render 'home/dashboard' %>
<div id="task">
  <h1>Update <%= @task.name %></h1>
  <%= render 'form', task: @task %>
  <%= form_with model: [@category, @task], local: true do |f| %>
  <div class="form-group">
      <%= f.check_box :accomplished %>
      <%= f.label :accomplished %>
  </div>
  <% end %>
  <%= link_to 'Back', category_tasks_path, class: 'btn btn-dark' %> 
</div>